{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "emitDeclarationOnly": false,
    "strict": true,
    "noEmit": true,
    "target": "ES2018",
    "jsx": "react",
    "baseUrl": ".",
    "rootDir": ".",
    "skipLibCheck": false,
    "noImplicitReturns": false
  },
  "exclude": ["dist", "**/*.typetest.ts*", "**/*.test-d.ts*"]
}
